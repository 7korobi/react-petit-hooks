{
  "name": "react-petit-hooks",
  "version": "0.3.27",
  "description": "petit store for react hooks",
  "main": "lib/index.js",
  "repository": "git@github.com:7korobi/react-petit-hooks.git",
  "homepage": "https://github.com/7korobi/react-petit-hooks",
  "bugs": {
    "url": "https://github.com/7korobi/react-petit-hooks/issues"
  },
  "author": "7korobi",
  "license": "MIT",
  "keywords": [
    "store",
    "localstorage",
    "sessionstorage",
    "href"
  ],
  "tags": [
    "store",
    "localstorage",
    "sessionstorage"
  ],
  "preferGlobal": true,
  "private": false,
  "scripts": {
    "test": "run-s compile format jest",
    "watch": "jest --watchAll --config config/jest.js --rootDir .",
    "deploy": "run-s format compile deploy:npm",
    "deploy:npm": "npm publish",
    "format": "prettier --no-semi --single-quote --print-width 100 --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "jest": "jest -u --config config/jest.js --rootDir .",
    "compile": "run-p compile:*",
    "compile:ts": "tsc -p .",
    "compile:scss": "node-sass src/browser.scss --precision 1 -o lib/",
    "type": "tsc -p . --noemit",
    "g": "run-p -n g:*",
    "g:npm": "npm i -g node-gyp grpc npm-check-updates"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.4",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "autoprefixer": "^9.8.6",
    "fake-indexeddb": "^3.1.2",
    "firebase": "^7.20.0",
    "jest": "^26.4.2",
    "jest-css-modules": "^2.1.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^4.0.1",
    "prettier": "^2.1.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "ts-jest": "^26.3.0",
    "yaml-jest": "^1.0.5"
  },
  "dependencies": {
    "@types/react": "^16.9.49",
    "@types/react-helmet": "^6.1.0",
    "@types/resize-observer-browser": "^0.1.3",
    "@types/ua-parser-js": "^0.7.33",
    "date-fns": "^2.16.1",
    "dexie": "^3.0.2",
    "fancy-date": "^0.3.12",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "sass-loader": "^10.0.2",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "ua-parser-js": "^0.7.21"
  }
}
