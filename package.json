    "@types/resize-observer-browser": "^0.1.3",
    "fake-indexeddb": "^3.1.2",
    "jest": "^26.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.2",
